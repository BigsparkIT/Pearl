{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "pearl/product-iframe",
  "title": "Product Iframe",
  "category": "embed",
  "icon": "format-image",
  "description": "Voegt een iframe toe met een productId als queryparameter.",
  "keywords": [
    "iframe",
    "product",
    "embed"
  ],
  "version": "1.0.0",
  "attributes": {
    "productId": {
      "type": "integer"
    },
    "offerType": {
      "type": "string",
      "default": "product"
    },
    "showOfferTypeSelector": {
      "type": "boolean",
      "default": "true"
    },
    "widgetId": {
      "type": "string"
    }
  },
  "supports": {
    "html": false
  },
  "editorScript": "file:./index.js"
}